{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":dependencyDashboard",
    ":disableRateLimiting",
    "docker:enableMajor",
    "docker:pinDigests",
    ":enablePreCommit"
  ],
  "onboarding": true,
  "requireConfig": "required",
  "timezone": "Europe/Amsterdam",
  "dependencyDashboardTitle": "Renovate Dashboard",
  "commitBodyTable": true,
  "rebaseWhen": "conflicted"
}
